{
  "name": "lil-networking",
  "packageManager": "yarn@1.22.19",
  "repository": "https://github.com/FoundryMakes/lil-networking",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/config/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --parallel --filter='!@foundrymakes/mobile'",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo clean",
    "lint": "turbo lint"
  },
  "dependencies": {
    "turbo": "^1.10.12",
    "typescript": "^5.1.6"
  }
}